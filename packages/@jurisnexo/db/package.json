{
  "name": "@jurisnexo/db",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "default": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --no-ignore src/",
    "lint:fix": "eslint --no-ignore src/ --fix",
    "migrate": "supabase db push",
    "migrate:create": "supabase migration new",
    "migrate:status": "supabase migration list",
    "generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.0"
  },
  "devDependencies": {
    "supabase": "^2.20.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
